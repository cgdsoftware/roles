{
    "name": "cgdsoftware/roles",
    "description": "Role Manager dependency for Laravel Enso",
    "keywords": [
        "cgdsoftware",
        "roles",
        "role-manager",
        "laravel-roles"
    ],
    "homepage": "https://github.com/cgdsoftware/Roles",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://cgdsoftware.com",
            "role": "Developer"
        }
    ],
    "require": {
        "cgdsoftware/core": "^8.0",
        "cgdsoftware/enums": "^2.0",
        "cgdsoftware/forms": "^4.0",
        "cgdsoftware/helpers": "^2.0",
        "cgdsoftware/menus": "^5.0",
        "cgdsoftware/permissions": "^5.0",
        "cgdsoftware/select": "^4.0",
        "cgdsoftware/migrator": "^2.0",
        "cgdsoftware/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Roles\\": "src/",
            "LaravelEnso\\Roles\\Database\\Factories\\": "database/factories/",
            "LaravelEnso\\Roles\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Roles\\AppServiceProvider",
                "LaravelEnso\\Roles\\EnumServiceProvider"
            ],
            "aliases": {}
        }
    }
}